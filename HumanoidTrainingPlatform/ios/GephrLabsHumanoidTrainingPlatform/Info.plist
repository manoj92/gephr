<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>CADisableMinimumFrameDurationOnPhone</key>
    <true/>
    <key>CFBundleDevelopmentRegion</key>
    <string>$(DEVELOPMENT_LANGUAGE)</string>
    <key>CFBundleDisplayName</key>
    <string>Gephr Labs - Humanoid Training Platform</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundlePackageType</key>
    <string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundleSignature</key>
    <string>????</string>
    <key>CFBundleURLTypes</key>
    <array>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>gephrlabs</string>
          <string>com.gephrlabs.humanoidtraining</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>exp+gephr-humanoid-training</string>
        </array>
      </dict>
    </array>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>ITSAppUsesNonExemptEncryption</key>
    <false/>
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>NSAppTransportSecurity</key>
    <dict>
      <key>NSAllowsArbitraryLoads</key>
      <false/>
      <key>NSAllowsLocalNetworking</key>
      <true/>
    </dict>
    <key>NSBluetoothAlwaysUsageDescription</key>
    <string>Gephr Labs uses Bluetooth to discover and connect to compatible humanoid robots for real-time training and control.</string>
    <key>NSBluetoothPeripheralUsageDescription</key>
    <string>Gephr Labs uses Bluetooth to communicate with robot hardware and training peripherals.</string>
    <key>NSCameraUsageDescription</key>
    <string>Gephr Labs uses your camera to capture precise hand gestures and movements for training humanoid robots. All processing is done locally on your device.</string>
    <key>NSFaceIDUsageDescription</key>
    <string>Allow $(PRODUCT_NAME) to use Face ID</string>
    <key>NSLocalNetworkUsageDescription</key>
    <string>Gephr Labs uses local network access to discover and connect to robots on your WiFi network.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>Gephr Labs uses location services to enhance robot training with environmental context and improve mapping accuracy.</string>
    <key>NSLocationAlwaysUsageDescription</key>
    <string>Allow $(PRODUCT_NAME) to access your location</string>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>Gephr Labs uses your location to provide context-aware robot training and environment mapping features.</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>Gephr Labs uses your microphone to record voice commands and audio cues for comprehensive robot training sessions.</string>
    <key>NSMotionUsageDescription</key>
    <string>Gephr Labs uses motion sensors to enhance hand tracking accuracy and provide precise gesture recognition for robot training.</string>
    <key>NSPhotoLibraryAddUsageDescription</key>
    <string>Gephr Labs can save your robot training sessions and gesture recordings to your device for future reference.</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>Gephr Labs can access your photo library to import reference materials and export training data.</string>
    <key>UIBackgroundModes</key>
    <array>
      <string>background-processing</string>
      <string>bluetooth-central</string>
      <string>background-processing</string>
      <string>bluetooth-central</string>
    </array>
    <key>UILaunchStoryboardName</key>
    <string>SplashScreen</string>
    <key>UIRequiredDeviceCapabilities</key>
    <array>
      <string>arm64</string>
    </array>
    <key>UIRequiresFullScreen</key>
    <false/>
    <key>UIStatusBarStyle</key>
    <string>UIStatusBarStyleDefault</string>
    <key>UISupportedInterfaceOrientations</key>
    <array>
      <string>UIInterfaceOrientationPortrait</string>
      <string>UIInterfaceOrientationPortraitUpsideDown</string>
    </array>
    <key>UISupportedInterfaceOrientations~ipad</key>
    <array>
      <string>UIInterfaceOrientationPortrait</string>
      <string>UIInterfaceOrientationPortraitUpsideDown</string>
      <string>UIInterfaceOrientationLandscapeLeft</string>
      <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
    <key>UIUserInterfaceStyle</key>
    <string>Dark</string>
    <key>UIViewControllerBasedStatusBarAppearance</key>
    <false/>
  </dict>
</plist>