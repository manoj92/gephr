# Gephr Labs Humanoid Training Platform - Environment Configuration

# API Configuration
API_BASE_URL=https://api.gephrlabs.com
API_VERSION=v1
API_TIMEOUT=30000

# Authentication
JWT_SECRET=your-jwt-secret-key-change-in-production
REFRESH_TOKEN_EXPIRY=7d
ACCESS_TOKEN_EXPIRY=15m

# Database
DATABASE_URL=https://api.gephrlabs.com/db
DATABASE_TIMEOUT=10000

# External Services
SENTRY_DSN=your-sentry-dsn
ANALYTICS_KEY=your-analytics-key
MIXPANEL_TOKEN=your-mixpanel-token

# Robot Communication
ROBOT_DISCOVERY_PORT=8080
ROBOT_COMMAND_TIMEOUT=5000
MAX_ROBOT_CONNECTIONS=5

# Hand Tracking
MEDIAPIPE_MODEL_URL=https://storage.googleapis.com/mediapipe-models/hand_landmarker/hand_landmarker/float16/1/hand_landmarker.task
HAND_TRACKING_CONFIDENCE_THRESHOLD=0.7
GESTURE_RECOGNITION_SENSITIVITY=0.8

# Storage & Caching
MAX_CACHE_SIZE=100
STORAGE_ENCRYPTION_ENABLED=true
DATA_RETENTION_DAYS=90

# Performance & Security
PERFORMANCE_MONITORING=true
SECURITY_AUDIT_LOGGING=true
CRASH_REPORTING=true
ANALYTICS_ENABLED=true

# Feature Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_OFFLINE_MODE=true
ENABLE_ADVANCED_MAPPING=true
ENABLE_MARKETPLACE=true
ENABLE_GAMIFICATION=true

# Development
DEBUG_MODE=false
VERBOSE_LOGGING=false
MOCK_ROBOT_CONNECTIONS=false
SKIP_ONBOARDING=false

# App Store / Play Store
APP_STORE_URL=https://apps.apple.com/app/gephr-labs-humanoid-training/id123456789
PLAY_STORE_URL=https://play.google.com/store/apps/details?id=com.gephrlabs.humanoidtraining

# Support & Legal
PRIVACY_POLICY_URL=https://gephrlabs.com/privacy
TERMS_OF_SERVICE_URL=https://gephrlabs.com/terms
SUPPORT_EMAIL=support@gephrlabs.com
WEBSITE_URL=https://gephrlabs.com